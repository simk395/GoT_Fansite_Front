{
  "name": "got-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "emoji-mart": "^2.11.0",
    "gulp": "^4.0.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "semantic": "0.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0",
    "semantic-ui-sass": "^2.4.1",
    "ui": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:sass": "node-sass sass/main.scss src/css/style.css -w",
    "compile:sass": "node-sass sass/main.scss src/css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.concat.css -o src/css/style.prefix.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "semantic-ui": "^2.4.2"
  }
}
